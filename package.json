{
  "name": "web-social-share",
  "version": "6.3.0",
  "description": "A Web Component for your PWA to share urls and content on social networks",
  "keywords": [
    "social",
    "share",
    "facebook",
    "instagram",
    "twitter",
    "whatsapp",
    "reddit",
    "pinterest",
    "pwa",
    "progressive-web-app",
    "progressive-web-apps",
    "stenciljs",
    "stencil"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "readme.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^1.8.7",
    "@stencil/dev-server": "latest",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "@stencil/utils": "latest",
    "@types/jest": "^25.1.2",
    "autoprefixer": "^9.7.4",
    "jest": "^25.1.0",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluster/web-social-share.git"
  },
  "author": "David Dal Busco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fluster/web-social-share"
  },
  "homepage": "https://websocialshare.com",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
